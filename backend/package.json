{
   "name": "backend",
   "private": true,
   "version": "1.0.0",
   "type": "commonjs",
   "main": "index.js",
   "scripts": {
      "dev": "cross-env NODE_ENV=development ts-node-dev --respawn --transpile-only src/server-auth.ts",
      "dev:sqlite": "cross-env NODE_ENV=development ts-node-dev --respawn --transpile-only src/server-auth.ts",
      "dev:auth": "cross-env NODE_ENV=development ts-node-dev --respawn --transpile-only src/server-auth.ts",
      "dev:mysql": "cross-env NODE_ENV=mysql ts-node-dev --respawn --transpile-only src/server.ts",
      "start": "cross-env NODE_ENV=production node dist/server.js",
      "build": "tsc && prisma generate",
      "start:prod": "cross-env NODE_ENV=production node dist/server.js",
      "start:dev": "cross-env NODE_ENV=development ts-node-dev --respawn --transpile-only src/server-auth.ts",
      "prisma:generate:sqlite": "prisma generate --schema=prisma/schema.sqlite.prisma",
      "prisma:generate:mysql": "prisma generate --schema=prisma/schema.mysql.prisma",
      "prisma:migrate:sqlite": "prisma migrate dev --schema=prisma/schema.sqlite.prisma",
      "prisma:migrate:mysql": "prisma migrate dev --schema=prisma/schema.mysql.prisma",
      "backup:sqlite": "powershell -ExecutionPolicy Bypass -File scripts/backup-sqlite.ps1",
      "setup:mysql": "powershell -ExecutionPolicy Bypass -File scripts/setup-mysql.ps1",
      "setup:aws-rds": "powershell -ExecutionPolicy Bypass -File scripts/setup-aws-rds.ps1",
      "test:mysql": "powershell -ExecutionPolicy Bypass -File scripts/test-mysql.ps1",
      "setup:env:dev": "powershell -ExecutionPolicy Bypass -File scripts/setup-env.ps1 development",
      "setup:env:prod": "powershell -ExecutionPolicy Bypass -File scripts/setup-env.ps1 production",
      "setup:env:test": "powershell -ExecutionPolicy Bypass -File scripts/setup-env.ps1 test",
      "predev:sqlite": "npm run backup:sqlite"
   },
   "keywords": [],
   "author": "",
   "license": "ISC",
   "dependencies": {
      "@prisma/client": "^6.15.0",
      "bcryptjs": "^3.0.2",
      "cors": "^2.8.5",
      "express": "^5.1.0",
      "helmet": "^8.1.0",
      "jsonwebtoken": "^9.0.2"
   },
   "devDependencies": {
      "@types/cors": "^2.8.19",
      "@types/express": "^5.0.3",
      "@types/jsonwebtoken": "^9.0.10",
      "cross-env": "^10.0.0",
      "prisma": "^6.15.0",
      "ts-node": "^10.9.2",
      "ts-node-dev": "^2.0.0",
      "typescript": "^5.9.2"
   }
}
